<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/head_color">

        <ImageView
            android:id="@+id/back"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:paddingRight="30dp"
            android:src="@drawable/icon_back" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="车队历史评分"
            android:textColor="@color/black"
            android:textSize="18sp" />
    </RelativeLayout>

    <com.xiaochao.lcrapiddeveloplibrary.viewtype.ProgressActivity
        android:id="@+id/activity_history_score_progress"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.xiaochao.lcrapiddeveloplibrary.widget.SpringView
            android:id="@+id/activity_history_score_springview"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/head_color"
                android:orientation="vertical"
                android:weightSum="10">


                <com.github.mikephil.charting.charts.LineChart
                    android:id="@+id/activity_chart_week"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginTop="10dp"
                    android:layout_weight="5" />

                <com.github.mikephil.charting.charts.LineChart
                    android:id="@+id/activity_chart_mounth"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginTop="10dp"
                    android:layout_weight="5" />

            </LinearLayout>

        </com.xiaochao.lcrapiddeveloplibrary.widget.SpringView>

    </com.xiaochao.lcrapiddeveloplibrary.viewtype.ProgressActivity>

</LinearLayout>

